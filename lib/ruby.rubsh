unset -v __Qfalse __Qtrue __Qnil

__Qfalse=0
__Qtrue=2
__Qnil=4

__rb_class_of () {
  local obj=$1
  local -A object=()

  case $obj in
    "$__Qnil"   ) __=$__rb_cNilClass    ;;
    "$__Qfalse" ) __=$__rb_cFalseClass  ;;
    "$__Qtrue"  ) __=$__rb_cTrueClass   ;;
    * )
      __get_object "$obj" object
      __=${object[class]}
      ;;
  esac
}

# vim: ft=sh
