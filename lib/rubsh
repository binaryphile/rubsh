#!/usr/bin/env bash

rubsh_lib=$(dirname "$(readlink -f "$BASH_SOURCE")")
export RUBSH_PATH=$rubsh_lib${RUBSH_PATH:+:}${RUBSH_PATH:-}:.
source "$rubsh_lib"/keywords.rubsh
unset -v rubsh_lib

strict_mode on
glob off

# $(require object    )
# $(require class     )
# $(require string    )
# $(require array     )
# $(require hash      )
# $(require io        )
# $(require file      )

$(__return_if_sourced)

usage=''
(( $# )) || __die "$usage"
unset -v usage

source "$@"
