#!/usr/bin/env bash

RUBSH_LIB=$(dirname "$(readlink -f "$BASH_SOURCE")")
export RUBSH_PATH+=${RUBSH_PATH:+:}$RUBSH_LIB:.
source "$RUBSH_LIB"/keywords.rubsh
unset -v RUBSH_LIB

$(require utils)
rubsh_initialize

$(require scope)
$(require values)
$(require predefined)
$(require object)

rubsh_cleanup

$(return_if_sourced)

bash --norc
